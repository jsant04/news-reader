{
  "version": 2,
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/dist",
  "installCommand": "npm install && cd server && npm install && cd ../web && npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "web/dist/index.html"
    }
  ]
}
